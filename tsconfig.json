{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "noEmit": true,
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
